{% layout none %}<!DOCTYPE html>
<!--[if IE 8]> <html class="lt-ie9" lang="en"> <![endif]-->
<!--[if (gt IE 8)|!(IE)]><!--> <html class="modern" lang="en"> <!--<![endif]-->
  <head>
    <!-- Basic page needs ================================================== -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>
      {{ shop.name }}
    </title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1">
    {% comment %}
    {% if settings.favicon != empty %}
    <link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" type="image/png" />
    {% endif %}
    {% endcomment %}
    <!-- Social ================================================== -->
    {% render 'social-meta-tags' %}

    {{ content_for_header }}
    {%- render '__css-root-vars' -%}
    <!-- CSS ================================================== -->
    {{ 'apps-password.css' | asset_url | stylesheet_tag }}
    {{ 'password.css' | asset_url | stylesheet_tag }}

    <!-- JS ================================================== -->
    {{ 'jquery.js' | asset_url | script_tag }}
    {{ 'countdown.js' | asset_url | script_tag }}


<script src="https://apis.google.com/js/platform.js" async defer></script>
<script
    type="text/javascript"
    async defer
    src="//assets.pinterest.com/js/pinit.js"
></script>
  </head>


  <body class="template-password">
    <div id="fb-root"></div>
	<script>(function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v3.0";
    fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));</script>
    {% section "password"%}
  </body>
</html>
